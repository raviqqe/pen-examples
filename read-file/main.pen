import System'Context { Context }
import System'File
import System'File'OpenOptions

readFile = \(ctx Context) none | error {
  f = File'Open(ctx, "foo.txt")?
  d = File'Read(ctx, f)?

  File'Write(ctx, File'StdOut(), d)?

  none
}

main = \(ctx Context) number {
  if _ = readFile(ctx) as none {
    0
  } else {
    1
  }
}
