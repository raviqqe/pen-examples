import Core'Number

mapNumbersToStrings = \(ns [number]) [string] {
  if [n, ...ns] = ns {
    [string; Number'String(n), ...mapNumbersToStrings(ns)]
  } else {
    [string;]
  }
}

mapNumbersToStringsLazily = \(ns [number]) [string] {
  [string;
    ...if [n, ...ns] = ns {
      [string; Number'String(n), ...mapNumbersToStringsLazily(ns)]
    } else {
      [string;]
    }
  ]
}
